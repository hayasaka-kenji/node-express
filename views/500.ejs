
<!doctype html>
<html lang="ja">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>500: Internal Server Error</title>
    <%- include('./_share/stylesheets'); %>
  </head>
  <body>
    <%- include('./_share/navbar'); %>
    <main role="main" class="container">

      <div class="border-bottom mt-5 mb-5">
        <h1>500: Internal Server Error</h1>
      </div>
      <h2>Request</h2>
      <table class="table table-bordered">
        <colgroup>
          <col style="width: 30%">
          <col style="width: 70%">
        </colgroup>
        <tr>
          <th>Request</th>
          <td>
            <span><%= data.method %></span>
            <span>&nbsp;</span>
            <span><%= data.url %></span>
            <span>&nbsp;</span>
            <span><%= (data.protocol || '').toUpperCase() %>/<%= data.version %></span>
          </td>
        </tr>
      </table>
      <% if (data.error) { %>
        <h2>Error</h2>
        <table class="table table-bordered">
          <colgroup>
            <col style="width: 30%">
            <col style="width: 70%">
          </colgroup>
          <tr>
            <th>Name</th>
            <td><%= data.error.name %></td>
          </tr>
          <tr>
            <th>Message</th>
            <td><%= data.error.message %></td>
          </tr>
          <tr>
            <th>Stack</th>
            <td><%= data.error.stack %></td>
          </tr>
        </table>
      <% } %>
    </main><!-- /.container -->
    <%- include('./_share/javascripts'); %>
  </body>
</html>
