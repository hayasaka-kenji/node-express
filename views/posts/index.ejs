<!doctype html>
<html lang="ja">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Welcom Page</title>
    <%- include('../_share/stylesheets'); %>
  </head>
  <body>
    <%- include('../_share/navbar'); %>
    <main role="main" class="container">

      <div class="border-bottom mt-5">
        <h1><%= title %></h1>
      </div>
      <dl class="row mt-5 mb-5">
        <dt class="col-3">Release date:</dt>
        <dd class="col-9"><%= published.toLocaleString() %></dd>
        <dt class="col-3">Author:</dt>
        <dd class="col-9"><%= authors %></dd>
        <dt class="col-3">Keyword:</dt>
        <dd class="col-9"><%= keywords %></dd>
      </dl>
      <p class="lead"><%= content %></p>

    </main><!-- /.container -->
    <%- include('../_share/javascripts'); %>
  </body>
</html>
